        Boolean check = 5<20;
        int number4= 50;
        int number5= 10;
        int number3= 15;
        int number6= 45;
        int number7= 11;
        //IF - ELSE IF - ELSE
        if (number3<number1&&number3<number2){
            System.out.println("verdadero");
        } else if (number3<number7) {
            System.out.println("ELSEIF");
        } else  {
            System.out.println("FIINNNNN");
        }
        // BUCLE-FOR
        for (int i=0;i<3;i++){
            System.out.println("Soy el mejor");
        }
        // SWITCH
        String wheather = "sunny";
        switch ( wheather) {
            case "sunny":
                System.out.println("SOLEADO");
            break;
            default:
                System.out.println("No se identifico");
