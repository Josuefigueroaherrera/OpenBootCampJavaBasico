package SmartDevice.Herencia;

public class SmartWatch {

    // 1. Atributos
    public String recordatorios;
    public String controlSue単o;
    public int llamadas;
    public int wifi;
    public int camara;
    double cronometro;
    public double temporizador;
    String despertador;
    int presionArterial;
    int contadorPasos;
    public double calorias;
    String alarma;
    int distancia;

    public SmartWatch () {

    }
    // 2. Constructores

    public SmartWatch (String recordatorios, String controlSue単o, double cronometro,
                       double temporizador, String despertador, int presionArterial, int contadorPasos,
                       double calorias, String alarma, int distancia){
        this.recordatorios= recordatorios;
        this.controlSue単o= controlSue単o;
        this.cronometro= cronometro;
        this.temporizador= temporizador;
        this.despertador= despertador;
        this.presionArterial= presionArterial;
        this.contadorPasos= contadorPasos;
        this.calorias= calorias;
        this.alarma= alarma;
        this.distancia= distancia;


    }

}
